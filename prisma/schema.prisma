generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Candidates {
  id        String   @id @default(uuid())
  email     String   @unique
  name      String
  notes     String
  phone     String
  step      String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
